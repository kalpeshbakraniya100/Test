@{
    ViewData["Title"] = "Dublicate Record Delete UI";
    Layout = null;
}

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

<link href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css" rel="stylesheet" />

<script src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
<script src="https://kit.fontawesome.com/e9b92a87d7.js" crossorigin="anonymous"></script>
<script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js "></script>
<div class="">
    <table id="dataTable" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>ClientCustomerID</th>
                <th>AccountID</th>
                <th>FirstName</th>
                <th>LastName</th>
                <th>Address1</th>
                <th>InsertedBy</th>
                <th>Count</th>
                <th>ShowData</th>
            </tr>
        </thead>
    </table>
</div>

<script>
    $(document).ready(function () {
        //alert("Test");
     var dtTable =   $('#dataTable').DataTable({
            "processing": true, // for show progress bar
            "serverSide": true, // for process server side
            "filter": false, // this is for disable filter (search box)
            "orderMulti": false, // for disable multiple column at once
            "ajax": {
                "url": "/home/dublicatedata",
                "type": "get",
                "datatype": "json"
            },
            "columns": [
                { "data": "ClientCustomerID", "autowidth": true },
                { "data": "AccountID", "autowidth": true },
                { "data": "FirstName", "autowidth": true },
                { "data": "LastName", "autowidth": true },
                { "data": "InsertedBy", "autowidth": true },
                { "data": "Address1", "autowidth": true },
                { "data": "cnt", "autowidth": true },
                {
                    "render": function (data, type, full, meta) { return '<a class="btn btn-info"   target="_blank" href="/Home/ShowData?ClientCustomerID=' + full.ClientCustomerID + '&AccountID=' + full.AccountID +' ">ShowData</a>'; }
                }
            ]
     });
    });
</script>
